{
  "include": ["src/**/*", "webpack/**/*"],
  "files": ["webpack.config.ts", "babel.config.js"],
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react-jsx",
    "noEmit": true,
    "typeRoots": ["node_modules/@types", "src/@types"],
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "isolatedModules": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowJs": true,
    "paths": {}
  }
}
