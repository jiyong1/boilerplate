{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "files": ["*.ts", "*.tsx"],
    "project": "./tsconfig.json"
  },
  "env": {
    "node": true
  },
  "extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
  "rules": {
    "no-console": "error",
    "no-debugger": "error"
  }
}
